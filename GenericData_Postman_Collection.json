    {
      "name": "Query Contacts - Multiple Filters",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"entityName\": \"Contact\",\n  \"filters\": [\n    {\n      \"field\": \"firstName\",\n      \"operator\": \"STARTS_WITH\",\n      \"value\": \"John\"\n    },\n    {\n      \"field\": \"email\",\n      \"operator\": \"ENDS_WITH\",\n      \"value\": \"@gmail.com\"\n    }\n  ],\n  \"page\": 0,\n  \"size\": 10\n}"
        },
        "url": {
          "raw": "http://localhost:8080/api/generic-data/query",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "api",
            "generic-data",
            "query"
          ]
        },
        "description": "Get contacts with firstName starting with 'John' AND email ending with '@gmail.com'"
      },
      "response": []
    },
    {
      "name": "Query Leads - Date Range",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"entityName\": \"Lead\",\n  \"filters\": [\n    {\n      \"field\": \"createdAt\",\n      \"operator\": \"GREATER_THAN_OR_EQUAL\",\n      \"value\": \"2024-01-01T00:00:00\"\n    },\n    {\n      \"field\": \"createdAt\",\n      \"operator\": \"LESS_THAN_OR_EQUAL\",\n      \"value\": \"2024-12-31T23:59:59\"\n    }\n  ],\n  \"page\": 0,\n  \"size\": 10,\n  \"sortBy\": \"createdAt\",\n  \"sortDirection\": \"DESC\"\n}"
        },
        "url": {
          "raw": "http://localhost:8080/api/generic-data/query",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "api",
            "generic-data",
            "query"
          ]
        },
        "description": "Get leads created in 2024 for the logged-in user"
      },
      "response": []
    },
    {
