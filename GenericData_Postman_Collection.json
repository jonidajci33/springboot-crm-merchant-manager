          "raw": "{\n  \"entityName\": \"DejavooCredentials\",\n  \"filters\": [\n    {\n      \"field\": \"apiKey\",\n      \"operator\": \"IS_NOT_NULL\"\n    }\n  ],\n  \"page\": 0,\n  \"size\": 10\n}"
        },
        "url": {
          "raw": "http://localhost:8080/api/generic-data/query",
          "protocol": "http",
          "host": [
            "localhost"
          ],
