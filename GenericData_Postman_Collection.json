{
  "info": {
    "_postman_id": "generic-data-api-2025",
    "name": "Generic Data Query API",
    "description": "Dynamic query endpoint for all entities with automatic user filtering and pagination",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Query Merchants - All",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"entityName\": \"Merchant\",\n  \"page\": 0,\n  \"size\": 10,\n  \"sortBy\": \"createdAt\",\n  \"sortDirection\": \"DESC\"\n}"
        },
        "url": {
          "raw": "http://localhost:8080/api/generic-data/query",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "api",
            "generic-data",
            "query"
          ]
        },
        "description": "Get all merchants for the logged-in user with pagination"
      },
      "response": []
    },
    {
      "name": "Query Merchants - With Filters",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"entityName\": \"Merchant\",\n  \"filters\": [\n    {\n      \"field\": \"merchantName\",\n      \"operator\": \"CONTAINS\",\n      \"value\": \"ABC\"\n    }\n  ],\n  \"page\": 0,\n  \"size\": 10,\n  \"sortBy\": \"merchantName\",\n  \"sortDirection\": \"ASC\"\n}"
        },
        "url": {
          "raw": "http://localhost:8080/api/generic-data/query",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "api",
            "generic-data",
            "query"
          ]
        },
        "description": "Get merchants where name contains 'ABC' for the logged-in user"
      },
      "response": []
    },
