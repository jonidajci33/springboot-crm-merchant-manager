          "raw": "{\n  \"entityName\": \"FileMetadata\",\n  \"filters\": [\n    {\n      \"field\": \"contentType\",\n      \"operator\": \"IN\",\n      \"value\": \"application/pdf,image/png,image/jpeg\"\n    }\n  ],\n  \"page\": 0,\n  \"size\": 15,\n  \"sortBy\": \"uploadedAt\",\n  \"sortDirection\": \"DESC\"\n}"
        },
        "url": {
          "raw": "http://localhost:8080/api/generic-data/query",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "api",
            "generic-data",
            "query"
          ]
        },
        "description": "Get recent PDF and image files uploaded by the logged-in user"
      },
      "response": []
    },
    {
      "name": "Query MerchantMiles - By Points",
      "request": {
