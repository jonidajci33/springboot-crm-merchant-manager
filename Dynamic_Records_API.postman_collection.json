											"value": "0"
										},
										{
											"key": "size",
											"value": "10"
										}
									]
								}
							},
							"status": "OK",
							"code": 200,
							"_postman_previewlanguage": "json",
							"header": [
								{
									"key": "Content-Type",
									"value": "application/json"
								}
							],
							"cookie": [],
							"body": "{\n    \"records\": [\n        {\n            \"recordId\": 1,\n            \"fields\": {\n                \"company_name_key_001\": \"Acme Corporation\",\n                \"contact_person_key_002\": \"John Smith\",\n                \"email_key_003\": \"john.smith@acme.com\",\n                \"phone_key_004\": \"555-0101\",\n                \"status_key_005\": \"active\",\n                \"revenue_key_006\": \"5000000\",\n                \"industry_key_007\": \"Technology\",\n                \"location_key_008\": \"New York, NY\"\n            }\n        }\n    ],\n    \"totalRecords\": 10,\n    \"currentPage\": 0,\n    \"pageSize\": 10,\n    \"totalPages\": 1\n}"
						}
					]
				},
				{
					"name": "Query Records with Filters",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}",
								"type": "text"
							},
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"menuId\": {{menuId}},\n    \"page\": 0,\n    \"size\": 10,\n    \"sortBy\": \"company_name_key_001\",\n    \"sortDirection\": \"ASC\",\n    \"filters\": [\n        {\n            \"fieldKey\": \"status_key_005\",\n            \"operator\": \"EQUALS\",\n            \"value\": \"active\"\n        }\n    ]\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/api/dynamic-records/query-simple",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"api",
								"dynamic-records",
								"query-simple"
							]
						},
						"description": "Query dynamic records with filtering, sorting, and pagination.\n\n**Filter Operators:**\n- EQUALS - Exact match (case-insensitive)\n- CONTAINS - Contains substring\n- STARTS_WITH - Starts with string\n- ENDS_WITH - Ends with string\n- GREATER_THAN - Numeric comparison\n- LESS_THAN - Numeric comparison\n\n**Multiple filters:** Applied with AND logic"
					},
					"response": []
				},
