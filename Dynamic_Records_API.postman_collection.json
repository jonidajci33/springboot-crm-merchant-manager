								"header": [
									{
										"key": "Authorization",
										"value": "Bearer {{token}}",
										"type": "text"
									}
								],
								"url": {
									"raw": "{{baseUrl}}/api/dynamic-records/template/4?page=0&size=10",
									"host": [
										"{{baseUrl}}"
									],
